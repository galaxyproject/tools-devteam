<?xml version="1.0"?>
<macros>
    <xml name="inputs_outputs">
        <inputs>
            <param format="tabular" help="" label="Select Input Data" name="in_file" type="data" />
            <yield />
        </inputs>
        <outputs>
            <data format="tabular" name="out_file" />
        </outputs>
    </xml>
    <xml name="requirements">
        <requirements>
            <requirement type="package" version="1.0.6">datamash</requirement>
        </requirements>
    </xml>
    <xml name="stdio">
        <stdio>
            <exit_code description="Error" level="fatal" range="1:" />
        </stdio>
    </xml>
</macros>